    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:deploy": "chmod +x deploy.sh && ./deploy.sh",
    "docker:deploy:prod": "chmod +x deploy-prod.sh && ./deploy-prod.sh"
